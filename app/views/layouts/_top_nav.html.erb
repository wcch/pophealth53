<div id="logOut">
  Welcome, <%= current_user.first_name.to_s %> <span class="sep">|</span>
  <a href="http://projectpophealth.org/faq.html">help</a> <span class="sep">|</span>
  <%= link_to 'account', edit_user_registration_path(current_user) %> <span class="sep">|</span>
  <% if current_user.admin? -%>
  <%= link_to 'admin', admin_users_path %> <span class="sep">|</span>
  <% end -%>
  <%= link_to 'logs', logs_index_path %> <span class="sep">|</span>
  <%= link_to 'logout', destroy_user_session_path, 'class' => 'log' %>
</div>