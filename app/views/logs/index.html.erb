<div id="dashboardContainer">
<!-- add a comment -->
  <div id="header">
    <div id="breadcrumbBox">
      <ul id="breadcrumbItems">
      <li>logs</li>
      </ul>
      <div id="logOut">
        Welcome, <%= current_user.first_name.to_s %> <span class="sep">|</span> <a href="http://projectpophealth.org/faq.html">help</a> <span class="sep">|</span> <%= link_to 'account', edit_user_registration_path(current_user) %> <span class="sep">|</span> <%= link_to 'logout', destroy_user_session_path, 'class' => 'log' %>
      </div>
    </div><!-- end #breadcrumbBox -->
  </div><!-- end #header -->
  <div id="pageContent">
    <h1>Logs</h1>
    <table id="patientTable">
      <% @logs.each do |log| -%>
      <tr>
        <td><%= log.created_at %></td>
        <td><%= log.username %></td>
        <td><%= log.event %></td>
      </tr>
      <% end -%>
    </table>

    <%= will_paginate @logs %>
  </div>
</div>