<%= semantic_remote_form_for @provider do |f| %>
  <%= f.inputs :title, :given_name, :family_name %>
  <%= f.input :specialty, :as => "select", :collection => Provider::Specialties.map { |k,v| [v, k]}  %>
  <%= f.input :organization %>
  <%= f.input :phone, :as => "phone" %>
  <%= f.input :tin %>
  <%= f.input :npi %>
  <%= f.buttons %>
<% end %>